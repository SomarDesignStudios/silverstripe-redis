{
    "name": "somardesignstudios/silverstripe-redis",
	"description": "A silverstripe module providing redis caching and session handling",
	"type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "user guide"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0",
        "predis/predis": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Somar\\Redis\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
  }
